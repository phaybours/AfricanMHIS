<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="5000" data-keyboard="true">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <div class="fill" style="background-image: url(''); background-image: url('../../Images/achilleamillefolium.jpg'); "></div>
            <div class="carousel-caption">
                <h1>Immune Booster</h1>
            </div>
        </div>
        <div class="item">
            <div class="fill" style="background-image: url(''); background-image: url('../../Images/Allium sepa.jpg'); "></div>
            <div class="carousel-caption">
                <h1>Improves Body Metabolism</h1>
            </div>
        </div>
        <div class="item">
            <div class="fill" style="background-image: url(''); background-image: url('../../Images/Lagenaria-LB1109-0109.jpg'); "></div>
            <div class="carousel-caption">
                <h1>
                    Natural Cleansing &amp; Anti-Oxidant
                </h1>
            </div>
        </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="icon-prev"></span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="icon-next"></span>
    </a>
</div>

